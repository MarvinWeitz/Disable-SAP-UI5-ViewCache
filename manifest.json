{
  "manifest_version": 3,
  "name": "Disable SAP UI Viewcache",
  "version": "1.0",
  "description": "Automatically adds sap-ui-xx-viewCache=false the URL",
  "permissions": ["tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "host_permissions": ["<all_urls>"]
}
